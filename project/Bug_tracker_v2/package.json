{
    "name": "bug-tracking-app",
    "version": "1.0.0",
    "description": "A web application for tracking and managing bugs in software projects.",
    "main": "index.js",
    "scripts": {
        "start": "concurrently \"npm run server\" \"npm run client\"",
        "server": "npm start --prefix Backend",
        "client": "npm start --prefix Frontend",
        "lint": "eslint './src/**/*.{js,jsx}'",
        "build": "npm run build --prefix Frontend",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d Frontend/build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "dependencies": {
        "axios": "^1.7.9",
        "concurrently": "^7.6.0",
        "eslint": "^8.57.0",
        "gh-pages": "^5.0.0",
        "react": "^18.3.1",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.11.0",
        "react-scripts": "^5.0.1",
        "semver": "^7.6.3",
        "web-vitals": "^4.2.4"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "keywords": [
        "bug-tracking",
        "react",
        "web-app",
        "project-management"
    ],
    "author": "Daniel Carpenco",
    "license": "ISC",
    "homepage": "https://danielcarpenco.github.io/Bug-Management-App",
    "devDependencies": {
        "@babel/core": "^7.26.0",
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11"
    }
}
